programa {
  funcao inicio() {
    real a, b, c 
    escreva ( "escolha o primeiro valor ")
    leia(a)
    escreva (" o segundo valor ")
    leia(b)
    escreva( "o terceiro valor ")
    leia (c)
    se ( a + b < c){
      escreva (" o primeiro valor mais o segundo valor é menor que o terceiro ")
    }
    se (a + b > c ){
      escreva(" o segundo valor mais o segundo valor é maior que o terceiro")
    }
  }
}
